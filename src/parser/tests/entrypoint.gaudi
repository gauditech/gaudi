entrypoint ItemEntry {
  target model Item
  identify with slug
  alias item
  response { name, price }
  list endpoint {}
  get endpoint {}
  update endpoint {}
  create endpoint {
    action {
      create Item {
        set foo bar
      }
      update Item {
        set foo 10
        reference factory through factory
      }
    }
  }
  entrypoint Review {
    target relation reviews
    create endpoint {}
  }
}
