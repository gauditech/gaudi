model Vehicle {
  field type { type string, default "car" }
  field active { type boolean, default true }
  field make { type string, nullable, default null }
  field notes { type string }
}

api {
  entrypoint Vehicle {
    create endpoint {
      action {
        create as vehicle {
          input { notes { default "Note not provided" } }
        }
      }
    }
  }
}
