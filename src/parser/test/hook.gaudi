model User {
  field fieldOne {
    type text
    validate {
      hook {
        arg fieldOne default
        source foo from "customValidateHook"
      }
    }
  }
  field fieldTwo {
    type text
    validate {
      hook {
        arg fieldTwo default
        inline `
          return fieldTwo.toLowerCase() === "something";
        `
      }
    }
  }
}
