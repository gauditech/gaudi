entrypoint ItemEntry {
  target model Item as item
  identify with slug
  response { name, price, brand { name } }
  list endpoint {}
  get endpoint {}
  update endpoint {}
  create endpoint {
    action {
      create item {
        set foo bar
        reference factory through factory
      }
      update item.parent {
        set foo 10
      }
    }
  }
  entrypoint Review {
    target relation reviews
    create endpoint {}
  }
}
