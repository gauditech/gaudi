model User {
  field fieldOne {
    type text
    validate {
      hook {
        arg fieldOne
        source "customValidateHook"
      }
    }
  }
  field fieldTwo {
    type text
    validate {
      hook {
        arg fieldTwo
        inline `
          return fieldTwo.toLowerCase() === "something";
        `
      }
    }
  }
}
