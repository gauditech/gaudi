{
  "name": "@gaudi/example-githubc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "// --- build": "",
    "build": "npm run build:hooks && npx gaudi-cli build && npm run build:web",
    "build:hooks": "copyfiles -u 1 \"hooks/**/*.js\" dist/hooks",
    "build:web": "copyfiles -u 2 \"src/web/**/*.*\" dist/web",
    "// --- dev": "",
    "dev": "concurrently --names hooks,gaudi,web,serve --prefix-colors magenta,blue,yellow,white \"npm run dev:hooks\" \"npx gaudi-cli dev --gaudi-dev\" \"npm run dev:web\" \"npm run dev:serve\"",
    "dev:hooks": "chokidar ./hooks  --debounce --initial --command \"npm run build:hooks\"",
    "dev:web": "chokidar ./src/web  --debounce --initial --command \"npm run build:web\"",
    "dev:serve": "node ./server.mjs ./dist/web",
    "// --- other": "",
    "clean": "rimraf ./dist"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@gaudi/engine": "file:../../dist",
    "is-ascii": "^1.0.0"
  },
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "copyfiles": "^2.4.1",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "rimraf": "^3.0.2"
  }
}
